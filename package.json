  },
  "homepage": "https://github.com/rawstock/rawstock-web#readme",
  "engines": {
    "node": "^8.0"
  },
  "dependencies": {
    "antd": "^3.6.1",
    "anymatch": "^2.0.0",
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "config": "^1.30.0",
    "echarts": "^4.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "force-scripts": "^1.1.8",
    "fork-ts-checker-webpack-plugin": "^0.2.10",
    "ip": "^1.1.5",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.4",
    "module-alias": "^2.0.3",
    "node-cache": "^4.2.0",
    "pm2": "^2.9.1",
    "prop-types": "^15.6.0",
    "pug": "^2.0.0-rc.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "tslib": "^1.9.2",
    "uuid": "^3.1.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/anymatch": "^1.3.0",
    "@types/classnames": "^2.2.3",
    "@types/koa": "^2.0.43",
    "@types/koa-bodyparser": "^3.0.26",
    "@types/koa-router": "^7.0.27",
    "@types/lodash": "^4.14.91",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/react-router": "^4.0.20",
    "@types/react-router-dom": "^4.2.3",
    "@types/winston": "^2.3.7",
    "autoprefixer": "^7.2.3",
    "chokidar": "^2.0.0",
    "concurrently": "^3.5.1",
    "core-js": "^2.5.7",
    "expose-loader": "^0.7.4",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "react-hot-loader": "^3.1.3",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.7.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "scripts": {
    "start": "pm2 kill && pm2 start tools/pm2.config.js --no-daemon",
    "dev": "rm -rf ./dist/* && npm run build && concurrently -p \"[{name}]\" -n \"TSC,NODE\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run watch\" \"npm run serve\"",
    "watch": "tsc -p . -w --pretty",
    "build": "tsc -p .",
    "build:libs": "rm -rf ./build/commonlib && force-scripts prebuild",
    "build:static": "NODE_ENV=production force-scripts build",
    "serve": "node dist/server"
  },
  "_moduleAliases": {
    "@lib": "./dist/lib",
    "@server": "./dist/server",
    "@client": "./dist/client",
    "@views": "./dist/client/views",
    "@utils": "./dist/client/utils"
  }
}
